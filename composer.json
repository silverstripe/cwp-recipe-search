{
    "name": "cwp/cwp-recipe-search",
    "type": "silverstripe-recipe",
    "description": "CWP search requirements recipe",
    "homepage": "https://www.cwp.govt.nz",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.3 || ^8.0",
        "silverstripe/framework": "^4.10",
        "silverstripe/recipe-plugin": "^1",
        "cwp/cwp-search": "1.x-dev",
        "silverstripe/fulltextsearch": "3.x-dev",
        "symbiote/silverstripe-queuedjobs": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3"
    },
    "suggest": {
        "silverstripe/textextraction": "Text Extraction API for SilverStripe CMS (mostly used with 'fulltextsearch' module)"
    },
    "extra": {
        "project-files": []
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "abandoned": "silverstripe/recipe-solr-search"
}
